{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./", // Changed from "./src" to "./"
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*",
    "generated/**/*" // Include generated directory
  ]
}
